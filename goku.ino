    lastFrameUpdate = currentTime;
    
    display.clearDisplay();
    display.drawBitmap(FRAME_X_OFFSET, 0, frames[animationSequence[currentSequenceIndex]], FRAME_WIDTH, FRAME_HEIGHT, WHITE);
    display.display();
    
    currentSequenceIndex = (currentSequenceIndex + 1) % SEQUENCE_LENGTH;
  }
}
